/* Insert your custom override CSS in this file */
 
 
@import 'https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css';


/* Police Bree Bold */
@font-face {
    font-family: 'breebold';
    src: url('fonts/breebold-webfont.woff2') format('woff2'),
         url('fonts/breebold-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'breeregular';
    src: url('fonts/BreeRegular.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}

/* barre du dessus */
 .navbar {
	border-bottom-color: #008461 !important; /* Vert logo Parc */
	border-bottom-width: 1px !important;
}

/* barre côté gauche */
 #sideBar{
	background-color: #5c5e63 !important;  /* Rouge logo parc */
 }